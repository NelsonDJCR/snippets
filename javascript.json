	"Ajax 💻": {
		"prefix": "ajax 🌌",
		"body": [
			"$.ajax({",
			"	type: \"POST\",",
			"	url: '$1',",
			"	data: $2,",
			"	headers: {'X-CSRF-TOKEN': $('meta[name='csrf-token']').attr('content')},",
			"	contentType: false,",
			"	success: function (r) {",
			"	  if (r == 1) {",
			"		$3",
			"	  }else if (r == 2) {",
			"		$4",
			"	  }",
			"	   else {",
			"		$5",
			"	  }",
			"	},",
			"  });",
			"return false;",
			"$2",
		],
		"description": "NelsonDJCR 🐉"
	},
