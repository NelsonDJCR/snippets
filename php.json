{
	"useController": {
		"prefix": "use.controller",
		"body": [
			"use App\\Http\\Controllers\\ $1;"
		],
		"description": "NelsonDJCR"
	},
	"Web": {
		"prefix": "route.get",
		"body": [
			"Route::get('/$route', [$Controller::class,'$function']);"
		],
		"description": "NelsonDJCR"
	},
	"web-post": {
		"prefix": "route.post",
		"body": [
			"Route::post('/$route', [$Controller::class,'$function']);"
		],
		"description": "NelsonDJCR"
	},	
	"php {}": {
		"prefix": "{{",
		"body": [
			"{{ $1 }}"
		],
		"description": "NelsonDJCR"
	},
	"post": {
		"prefix": "post",
		"body": [
			"$$_POST['$1'];"
		],
		"description": "NelsonDJCR"
	},
	"var": {
		"prefix": "var",
		"body": [
			"$$1;"
		],
		"description": "NelsonDJCR"
	},
	"Print Y": {
		"prefix": "print_y",
		"body": [
			"echo '<pre>'; die(print_r($$1)); "
		],
		"description": "NelsonDJCR"
	},
	"Die": {
		"prefix": "die",
		"body": [
			"die('$1');$2",
		],
		"description": "NelsonDJCR"
	},
	"echo": {
		"prefix": "echo",
		"body": [
			"echo \"$1\";",
		],
		"description": "NelsonDJCR"
	},
	"try": {
		"prefix": "try.",
		"body": [
			"try{",
			"	  $1",
			"} catch (\\Illuminate\\Database\\QueryException $$ex) {",
			"	return response()->json(['msg' => $$ex->getMessage()$2]);",
			"}",
		],
		"description": "NelsonDJCR"
	},
	"update": {
		"prefix": "update.",
		"body": [
			"DB::table('$1')",
			"	 ->where('id', $$r->$2)",
			"	 ->update([",
			"	  	'$3' =>$4 ,",
			"	  ]);",

		],
		"description": "NelsonDJCR"
	},
	"query": {
		"prefix": "query.",
		"body": [
			"DB::table('$1')->where('id', $$r->$2)->$get()"
		],
		"description": "NelsonDJCR"
	},
	"insert": {
		"prefix": "insert.",
		"body": [
			" DB::table('$1')->insert([",
				"'$2' => $3,",
			"]);"
		],
		"description": "NelsonDJCR"
	},
	//**** For blade ****//
	"select": {
		"prefix": "-select",
		"body": [
			"->select('$1')$2",
		],
		"description": "NelsonDJCR"
	},
	"take": {
		"prefix": "-take",
		"body": [
			"->take($1)$2",
		],
		"description": "NelsonDJCR"
	},
	"Request $r": {
		"prefix": ".request $r",
		"body": [
			"Request $$r",
		],
		"description": "NelsonDJCR"
	},
	"->with": {
		"prefix": "-with",
		"body": [
			"->with('$1')$2",
		],
		"description": "NelsonDJCR"
	},
	"where": {
		"prefix": "->where",
		"body": [
			"->where('$1',$2)$3",
		],
		"description": "BLADE Where"
	},
	"first": {
		"prefix": "-first",
		"body": [
			"->first()",
		],
		"description": "NelsonDJCR"
	},
	"get": {
		"prefix": "-get()",
		"body": [
			"->get()",
		],
		"description": "NelsonDJCR"
	},
	"leftjoin": {
		"prefix": "-leftjoin",
		"body": [
			"->leftjoin(\"$1\", \"$1.$2\", \"$3\")$4",
		],
		"description": "NelsonDJCR"
	},
	"save();": {
		"prefix": "-save();",
		"body": [
			"->save();",
		],
		"description": "NelsonDJCR"
	},
	
	"Json": {
		"prefix": "json",
		"body": [
			"return response()->json([",
			"	'$1' => '$2'",
			"]);"
		],
		"description": "NelsonDJCR"
	},
	"ResponseJson": {
		"prefix": "json-response",
		"body": [
			"return response()->json([",
			"	request()->all()$1",
			"]);"
		],
		"description": "NelsonDJCR"
	},
	"$table": {
		"prefix": "$table->",
		"body": [
			"$$table->$1"
		],
		"description": "NelsonDJCR"
	},
	"useModel": {
		"prefix": "use.model",
		"body": [
			"use App\\Models\\ $1;"
		],
		"description": "NelsonDJCR"
	},

    // Migrations
    "$table->": {
		"prefix": "table->",
		"body": [
			"$$table->$1",
		],
		"description": "NelsonDJCR"
	},
    "ForeingKey": {
		"prefix": "-foreing",
		"body": [
			"$$table->foreignId('$1')->nullable()->constrained('$2');$3"
		],
		"description": "NelsonDJCR"
	},
    "nullable": {
		"prefix": "-nullable()",
		"body": [
			"->nullable()$1"
		],
		"description": "NelsonDJCR"
	},
    "after": {
		"prefix": "-after()",
		"body": [
			"->after('$1')"
		],
		"description": "NelsonDJCR"
	},
    "default": {
		"prefix": "-default()",
		"body": [
			"->default('$1')$2"
		],
		"description": "NelsonDJCR"
	},
    "String": {
		"prefix": "-string()",
		"body": [
			"$$table->string('$1')$2;$3"
		],
		"description": "NelsonDJCR"
	},
    "integer": {
		"prefix": "-integer()",
		"body": [
			"$$table->integer('$1')$2;$3"
		],
		"description": "NelsonDJCR"
	},
    "text": {
		"prefix": "-text()",
		"body": [
			"$$table->text('$1')$2;$3"
		],
		"description": "NelsonDJCR"
	},
    "drop": {
		"prefix": "-drop()",
		"body": [
			"$$table->dropColumn('$1')$2;$3"
		],
		"description": "NelsonDJCR"
	},
    
    
    "dropForeing": {
		"prefix": "-dropForeing",
		"body": [
			"$$table->dropForeign(['$1']);",
			"$$table->dropColumn('$1');"
		],
		"description": "NelsonDJCR"
	},
	
}