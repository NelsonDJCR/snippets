			"	  ]);",

		],
		"description": "Update Blade"
	},
	"query": {
		"prefix": "query.",
		"body": [
			"DB::table('$1')->where('id', $$r->$2)->$get()"

		],
		"description": "Update Blade"
	},
	"insert": {
		"prefix": "insert.",
		"body": [
			" DB::table('$1')->insert([",
				"'$2' => $3,",
			"]);"

		],
		"description": "Insery Blade"
	},

	
	
	
		  
		  
	  





